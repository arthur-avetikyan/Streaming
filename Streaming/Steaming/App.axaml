             xmlns:local="clr-namespace:Streaming"
             x:Class="Streaming.App">
  <Application.DataTemplates>
    <local:ViewLocator/>
  </Application.DataTemplates>

  <Application.Styles>
    <StyleInclude Source="avares://Avalonia.Themes.Default/DefaultTheme.xaml"/>
    <StyleInclude Source="avares://Avalonia.Themes.Default/Accents/BaseLight.xaml"/>
    <StyleInclude Source="avares://Streaming/Styles/Common.axaml" />
    <StyleInclude Source="avares://Streaming/Styles/Dialogs.axaml"/>
    <StyleInclude Source="avares://Streaming/Styles/MainWindow.axaml "/>
  </Application.Styles>
</Application>
